{
    "plugins": [
        ["transform-object-rest-spread"],
        ["dynamic-import-webpack"],
        ["syntax-dynamic-import"],
        ["transform-runtime", {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
        }],
        ["babel-plugin-styled-components"]
    ],
    
    "presets": [
        "react",
        [
            "env",
            {
                "targets": {
                    "browsers": ["last 1 version", "ie >= 11"]
                }
            }
        ]
    ]
}
